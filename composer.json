{
    "name": "jorgecortesdev/cc-strategy-pattern",
    "type": "project",
    "description": "Este repositorio contiene el código de ejemplo utilizado en el video del patrón adaptador de mi canal de youtube.",
    "keywords": ["php"],
    "require": {
        "php": "^8.2"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.4"
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "tests": "vendor/bin/phpunit"
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
